<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
    <link rel="stylesheet" href="/css/clientes.css" />
    <%- include("partials/head") %>
  </head>
  <body>
    <%- include("partials/navigation") %>
    <div class="container">
      <div class="container mt-5">
        <form class="form-inline">
          <label for="razon-social" class="mr-2"
            >Ingrese la razón social:</label
          >
          <input
            type="text"
            id="razon-social"
            class="form-control"
            oninput="buscarRazonesSociales()"
          />
        </form>
      </div>
      <div id="resultados-busqueda"></div>
      <div id="razon-social-resultados"></div>

      <div id="output" class="mt-3"></div>

      <div id="mensaje" class="mt-3">
        <div class="card text-white">
          <div class="card-body">
            <!-- Aquí se mostrará el mensaje de error -->
          </div>
        </div>
      </div>
    </div>
    <br />
    <br />
    <%- include("partials/footer") %>
    <script src="/script/buscador.js"></script>
    <script src="/script/nigthday.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>
